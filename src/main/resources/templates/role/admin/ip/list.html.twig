{% extends '../base.html.twig' %}
{% block body %}


    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h3>IP Address
                        <button type="button" class="btn btn-primary" onclick="location.href='{{ path('/admin/ip/create') }}'" style="float:right">
                        New IP Address
                        </button>
                    </h3>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    {% if message|length > 0 %}
                        <br/>
                        <div class="alert alert-danger" id="errorDiv">
                            <span class="close" onclick="this.parentElement.style.display='none';">&times;</span>
                            <strong> {{ message }} </strong>
                        </div>
                    {% endif %}
                    <p class="text-muted font-13 m-b-30"></p>
                    <table id="datatable" class="table table-striped table-bordered">
                        <h4>CSMC</h4>
                        <thead>
                        <tr role="row">
                            <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1"
                                aria-label="Name: activate to sort column descending" style="width: 189px;"
                                aria-sort="ascending">IP Address
                            </th>
                            <th style="width:20%" class="sorting" tabindex="0" aria-controls="datatable" rowspan="1"
                                colspan="1">Room
                            </th>
                            <th style="width:20%" class="sorting" tabindex="0" aria-controls="datatable" rowspan="1"
                                colspan="1">Blocked
                            </th>
                            <th style="width:20%" class="sorting" tabindex="0" aria-controls="datatable" rowspan="1"
                                colspan="1">Edit
                            </th>

                        </thead>


                        <tbody>
                        {% for ip in csmc %}
                            <tr>
                                <td>{{ ip.ipAddress }}</td>
                                <td>{% if ip.building != NULL %}
                                        {{ ip.building~" "~ ip.floor~"."~ip.classRoomNumber }}
                                    {#{% else %}#}
                                        {#{{ ip.room }}#}
                                    {% endif %}
                                </td>
                                <td>
                                    {% if ip.blocked == 1 %}
                                        Yes
                                    {% else %}
                                        No
                                    {% endif %}
                                </td>
                                <td>
                                    <button type="button" class="btn btn-success"
                                            onclick="location.href='{{ path('/admin/ip/edit/' ~ ip.ipId) }}'">Edit
                                    </button>
                                </td>


                            </tr>

                        {% endfor %}

                    </table>
                </div>

